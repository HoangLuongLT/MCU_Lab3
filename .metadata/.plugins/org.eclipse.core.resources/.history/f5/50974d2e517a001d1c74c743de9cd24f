#include "fsm_pedestrian.h"
#include "global.h"
#include "output_display.h"
#include "software_timer.h"
#include "button.h"

void fsm_pedestrian_processing(){
	switch (status) {
		case PED_STOP:
			pedStop();
			break;
		case PED_GO:
			pedGo();
			break;
		default:
			break;
	}
}

